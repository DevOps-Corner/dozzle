{
 "name": "dozzle",
 "version": "2.1.4",
 "description": "Realtime log viewer for docker containers. ",
 "scripts": {
  "prestart": "yarn clean",
  "watch": "npm-run-all -p watch:*",
  "watch:assets": "webpack --mode=development --watch",
  "watch:server": "reflex -c .reflex",
  "dev": "npm-run-all -p dev-server watch:server",
  "dev-server": "vite serve assets --open",
  "prebuild": "yarn clean",
  "build": "yarn webpack --mode=production",
  "clean": "rm -rf static/ a_main-packr.go",
  "release": "release-it",
  "test": "TZ=UTC jest",
  "integration": "docker-compose -f integration/docker-compose.test.yml up --build --force-recreate --exit-code-from integration"
 },
 "repository": {
  "type": "git",
  "url": "git+https://github.com/amir20/dozzle.git"
 },
 "author": "",
 "license": "ISC",
 "bugs": {
  "url": "https://github.com/amir20/dozzle/issues"
 },
 "homepage": "https://dozzle.dev",
 "dependencies": {
  "ansi-to-html": "^0.6.14",
  "buefy": "^0.9.3",
  "bulma": "^0.9.1",
  "date-fns": "^2.16.1",
  "dompurify": "^2.1.1",
  "hotkeys-js": "^3.8.1",
  "lodash-es": "^4.17.15",
  "semver": "^7.3.2",
  "splitpanes": "^3.0.1",
  "store": "^2.0.12",
  "vue": "^3.0.0-rc.5",
  "vue-meta": "^2.4.0",
  "vue-router": "^4.0.0-beta.4"
 },
 "devDependencies": {
  "@babel/core": "^7.11.6",
  "@babel/plugin-transform-runtime": "^7.11.5",
  "@vue/component-compiler-utils": "^3.2.0",
  "@vue/test-utils": "^1.1.0",
  "babel-core": "^7.0.0-bridge.0",
  "babel-jest": "^26.5.2",
  "babel-preset-env": "^1.7.0",
  "caniuse-lite": "^1.0.30001146",
  "eventsourcemock": "^2.0.0",
  "husky": "^4.3.0",
  "jest": "^26.5.2",
  "jest-serializer-vue": "^2.0.2",
  "lint-staged": "^10.4.0",
  "mini-css-extract-plugin": "^1.0.0",
  "npm-run-all": "^4.1.5",
  "postcss-cssnext": "^3.1.0",
  "postcss-import": "^12.0.1",
  "prettier": "^2.1.2",
  "release-it": "^14.0.4",
  "sass": "^1.27.0",
  "vite": "^1.0.0-rc.4",
  "vue-hot-reload-api": "^2.3.4",
  "vue-jest": "^3.0.7"
 },
 "husky": {
  "hooks": {
   "pre-commit": "lint-staged"
  }
 },
 "lint-staged": {
  "*.{js,vue,css}": [
   "prettier --write"
  ]
 },
 "release-it": {
  "github": {
   "release": true
  },
  "npm": {
   "publish": false
  }
 }
}
